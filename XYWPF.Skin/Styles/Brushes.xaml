<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:XYWPF.Skin.Styles">
    <SolidColorBrush x:Key="BorderBrush" Color="{DynamicResource BorderColor}"></SolidColorBrush>
    <SolidColorBrush x:Key="ButtonBackgroundBrush" Color="{DynamicResource ButtonBackgroundColor}"></SolidColorBrush>
    <SolidColorBrush x:Key="ButtonForegroundBrush" Color="{DynamicResource ButtonForeColor}"></SolidColorBrush>
</ResourceDictionary>